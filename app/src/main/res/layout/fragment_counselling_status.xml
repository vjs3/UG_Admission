<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textViewNameOfApplicant"
            android:text="Name of Applicant"
            android:layout_marginTop="24dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />

        <View
            android:id="@+id/line_below_name_of_applicant_counsellingStatus"
            android:layout_below="@+id/textViewNameOfApplicant"
            android:layout_width="match_parent"
            android:layout_height="6dip"
            android:background="@color/black"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:layout_centerHorizontal="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textViewApplicationId"
            android:text="Application Id"
            android:layout_below="@+id/line_below_name_of_applicant_counsellingStatus"
            android:layout_centerHorizontal="true"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />

        <View
            android:id="@+id/line_below_name_of_applicant"
            android:layout_below="@+id/textViewApplicationId"
            android:layout_width="match_parent"
            android:layout_height="6dip"
            android:background="@color/black"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:layout_centerHorizontal="true"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textViewPreferredBranchStatus"
            android:layout_below="@+id/line_below_name_of_applicant"
            android:layout_centerHorizontal="true"
            android:text="Your Preferred Branch Status"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />

        <View
            android:id="@+id/line_below_preferred_branch_status"
            android:layout_below="@+id/textViewPreferredBranchStatus"
            android:layout_width="320dp"
            android:layout_height="6dip"
            android:background="@android:color/black"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:layout_centerHorizontal="true"/>


        <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/tableLayout_counsellingStatus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/line_below_preferred_branch_status"
            android:weightSum="10"
            >

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <TextView
                    android:text="S. No."
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
                <TextView
                    android:text="Preferred Branches"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_column="2"
                    android:layout_weight="5"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
                <TextView
                    android:text="Seat Status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_column="3"
                    android:layout_weight="3"
                    android:gravity="center"
                    android:padding="5dp"
                    android:textSize="18sp"
                    android:background="@drawable/border_cell"
                    android:textStyle="bold"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
            </TableRow>
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <TextView
                    android:id="@+id/cell_11"
                    android:text="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
                <TextView
                    android:id="@+id/cell_12"
                    android:text="CSE"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_column="2"
                    android:layout_weight="5"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
                <TextView
                    android:id="@+id/cell_13"
                    android:text="Confirmed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_column="3"
                    android:layout_weight="3"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
            </TableRow>
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <TextView
                    android:id="@+id/cell_21"
                    android:text="2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
                <TextView
                    android:id="@+id/cell_22"
                    android:text="MME"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_column="2"
                    android:layout_weight="5"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
                <TextView
                    android:id="@+id/cell_23"
                    android:text="WL-2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_column="3"
                    android:layout_weight="3"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
            </TableRow>
            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <TextView
                    android:id="@+id/cell_31"
                    android:text="3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
                <TextView
                    android:id="@+id/cell_32"
                    android:text="CCE"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_column="2"
                    android:layout_weight="5"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
                <TextView
                    android:id="@+id/cell_33"
                    android:text="WL-19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_column="3"
                    android:layout_weight="3"
                    android:gravity="center"
                    android:padding="5dp"
                    android:background="@drawable/border_cell"
                    android:textSize="18sp"
                    android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />
            </TableRow>

        </TableLayout>
        <View
            android:id="@+id/line_below_table_layout"
            android:layout_width="match_parent"
            android:layout_height="6dip"
            android:background="@color/black"
            android:layout_gravity="center"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/tableLayout_counsellingStatus"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textViewRoundStatus"
            android:layout_below="@+id/line_below_table_layout"
            android:layout_centerHorizontal="true"
            android:text="Updates"
            android:textSize="20sp"
            android:textStyle="bold"
            android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />

        <View
            android:id="@+id/line_below_round_status"
            android:layout_width="320dp"
            android:layout_height="6dip"
            android:background="@color/black"
            android:layout_gravity="center"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/textViewRoundStatus"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="15dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textViewUpdates_counsellingStatus"
            android:layout_below="@+id/line_below_round_status"
            android:layout_centerHorizontal="true"
            android:text="Your CCE waitlist has been upgraded to seat 13"
            android:gravity="center"
            android:textSize="20sp"
            android:layout_marginRight="25dp"
            android:layout_marginLeft="25dp"
            android:textAppearance="@style/Base.Widget.AppCompat.Spinner.Underlined" />

        <View
            android:id="@+id/line_below_status"
            android:layout_width="match_parent"
            android:layout_height="6dip"
            android:background="@color/black"
            android:layout_gravity="center"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/textViewUpdates_counsellingStatus"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="10dp"/>

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Click Here to Proceed"
            android:id="@+id/buttonClickHereToProceeed_counsellingStatus"
            android:layout_below="@+id/line_below_status"
            android:layout_centerHorizontal="true"
            android:background="@android:color/black"
            android:textColor="@android:color/white"
            android:elevation="@dimen/abc_control_corner_material"
            android:focusable="true"
            android:clickable="true"
            android:paddingRight="20dp"
            android:paddingLeft="20dp" />
    </RelativeLayout>

</ScrollView>